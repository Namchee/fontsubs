{
  "name": "@namchee/astro-subfont",
  "version": "0.1.0",
  "description": "Generate optimal font subsets for your Astro-powered websites",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "postinstall": "npm install -g subfont"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "keywords": [
    "astro",
    "withastro",
    "optimization",
    "font",
    "astro-integration"
  ],
  "author": {
    "email": "hello@namchee.dev",
    "name": "Cristopher",
    "url": "https://www.namchee.dev"
  },
  "license": "MIT",
  "dependencies": {
    "astro-integration-kit": "^0.12.0",
    "kleur": "^4.1.5"
  },
  "peerDependencies": {
    "astro": "^4"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^11.1.6",
    "astro": "^4.7.0",
    "auto": "^11.1.6",
    "tsup": "^8.0.2"
  },
  "repository": "Namchee/fontsubs",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "auto": {
    "plugins": [
      "npm",
      "conventional-commits",
      "released"
    ],
    "onlyPublishWithReleaseLabel": true
  }
}
